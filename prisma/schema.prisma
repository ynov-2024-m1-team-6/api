// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  name        String
  firstName   String
  mail        String   @unique
  password    String
  adress      String
  zipCode     Int
  city        String
  phoneNumber Int
  isAdmin     Boolean   @default(false)
}
